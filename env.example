# Supabase 설정
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# 데이터베이스 암호화 키 (실제 운영환경에서는 안전하게 관리)
DATABASE_ENCRYPTION_KEY=your_32_character_encryption_key
NEXT_PUBLIC_ENCRYPTION_KEY=your_32_character_encryption_key

# 보안 설정
NEXT_PUBLIC_SECURITY_MONITORING_ENABLED=true
NEXT_PUBLIC_RATE_LIMITING_ENABLED=true
NEXT_PUBLIC_AUDIT_LOGGING_ENABLED=true

# 세션 관리
SESSION_SECRET=your_session_secret_key
SESSION_MAX_AGE=86400

# 보안 헤더
NEXT_PUBLIC_CSP_ENABLED=true
NEXT_PUBLIC_HSTS_ENABLED=true

# 애플리케이션 설정
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development

